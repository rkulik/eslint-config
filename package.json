{
  "name": "@rkulik/eslint-config",
  "version": "1.0.0",
  "description": "René's ESLint configuration",
  "keywords": [
    "eslint",
    "config",
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkulik/eslint-config.git"
  },
  "license": "MIT",
  "author": "René Kulik <rene@kulik.io> (https://www.kulik.io/)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --dts",
    "lint": "eslint . -c eslint.config.js",
    "test": "jest",
    "prepare": "husky"
  },
  "bugs": {
    "url": "https://github.com/rkulik/eslint-config/issues"
  },
  "homepage": "https://github.com/rkulik/eslint-config#readme",
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=20.12.2",
    "pnpm": ">=9.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@rkulik/prettier-config": "^1.1.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@eslint/js": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "^8.13.0"
  },
  "peerDependencies": {
    "eslint": "^9.14.0"
  },
  "prettier": "@rkulik/prettier-config"
}
